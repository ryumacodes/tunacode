{
    "default_model": "openrouter:openai/gpt-4.1",
    "env": {
        "ANTHROPIC_API_KEY": "",
        "GEMINI_API_KEY": "",
        "OPENAI_API_KEY": "",
        "OPENROUTER_API_KEY": ""
    },
    "settings": {
        "max_retries": 10,
        "max_iterations": 40,
        "request_delay": 0.0,
        "global_request_timeout": 90.0,
        "guide_file": "AGENTS.md",
        "fallback_response": true,
        "fallback_verbosity": "normal",
        "context_window_size": 200000,
        "enable_streaming": true,
        "theme": "dracula",
        "max_command_output": 5000,
        "max_files_in_dir": 50,
        "max_tokens": null,
        "ripgrep": {
            "timeout": 10,
            "max_results": 100,
            "enable_metrics": false
        }
    }
}
